[ignore]
./test/.*
./static/.*
./dist/.*
./.vscode/.*
./.git/.*
.*/node_modules/.*
./build/.*

[include]

[libs]
flow-typed

[options]
unsafe.enable_getters_and_setters=true
module.name_mapper='^root\(.*\)$' -> '<PROJECT_ROOT>/\1'
module.name_mapper='^src\(.*\)$' -> '<PROJECT_ROOT>/src\1'
module.name_mapper='^services\(.*\)$' -> '<PROJECT_ROOT>/src/services\1'
module.name_mapper='^actions\(.*\)$' -> '<PROJECT_ROOT>/src/actions\1'
module.name_mapper='^mixins\(.*\)$' -> '<PROJECT_ROOT>/src/mixins\1'
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/src/components\1'
module.name_mapper='^directives\(.*\)$' -> '<PROJECT_ROOT>/src/directives\1'
module.name_mapper='^stores\(.*\)$' -> '<PROJECT_ROOT>/src/stores\1'
module.name_mapper='^assets\(.*\)$' -> '<PROJECT_ROOT>/src/assets\1'
module.name_mapper='^shared\(.*\)$' -> '<PROJECT_ROOT>/src/components/shared\1'
module.system.node.resolve_dirname=src
