{
  "extends": "airbnb-base",
  "env": {
    "mocha": true,
    "es6": true,
    "browser": true,
    "node": true,
  },
  "globals": {
    "expect": true,
    "sinon": true,
    "assert": true
  },
  "rules": {
    "import/extensions": ["error", "always", {
      "js": "never",
      "vue": "never"
    }],
    "no-unused-expressions": "off"
  }
}
